{
    "defaultSeverity": "error",
    "extends": [
        "tslint:recommended"
    ],
    "jsRules": {},
    "rules": {
        "quotemark": [
            true,
            "single"
        ],
        "ordered-imports": false,
        "member-access": [
            true,
            "no-public"
        ],
        "trailing-comma": false,
        "no-console": [
            true,
            "error"
        ],
        "no-string-literal": false,
        "arrow-parens": [
            true,
            "ban-single-arg-parens"
        ],
        "forin": false,
        "curly": [
            true,
            "as-needed"
        ],
        "object-literal-sort-keys": false
    },
    "rulesDirectory": []
}
